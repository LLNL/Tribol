PROJECT_NAME  = @PROJECT_NAME@
PROJECT_BRIEF = "Contact Interface Physics Library"
INPUT = @PROJECT_SOURCE_DIR@/src
RECURSIVE = YES
GENERATE_XML = YES
USE_MATHJAX = YES
MATHJAX_RELPATH = https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.2
FILE_PATTERNS += *.md *.markdown *.cpp *.hpp
